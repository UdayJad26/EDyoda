   "source": [
    "## Write a Python program to reverse a string.\n",
    "\n",
    "\n",
    "\n",
    "### Sample String : \"1234abcd\"\n",
    "\n",
    "### Expected Output : \"dcba4321\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "f467b53e",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1234abcd\n",
      "dcba4321\n"
     ]
    }
   ],
   "source": [
    "str1=str(input())\n",
    "\n",
    "def reverseString(s):\n",
    "    r=\"\"\n",
    "    for i in range(len(s)-1,-1,-1):\n",
    "        r=r+s[i]\n",
    "        \n",
    "    return r\n",
    "\n",
    "ans=reverseString(str1)\n",
    "print(ans)"
   ]
  },
