   "source": [
    "## Write a Python program that accepts a word from the user and reverse it.\n",
    "\n",
    "### Sample Test Case\n",
    "\n",
    "##### Input : Edyoda\n",
    "\n",
    "#### output: adoydE\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "bb3d8e5a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Edyoda\n",
      "adoydE\n"
     ]
    }
   ],
   "source": [
    "s1=str(input())\n",
    "s2=\"\"\n",
    "for i in range(len(s1)-1,-1,-1):\n",
    "    s2=s2+s1[i]\n",
    "    \n",
    "print(s2)"
   ]
